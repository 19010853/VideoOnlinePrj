{
    "version": 3,
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/src/index.ts"
        }
    ],
    "builds": [
        {
            "src": "src/index.ts",
            "use": "@vercel/node",
            "config": {
                "includeFiles": ["src/**/*.ts"]
            }
        }
    ],
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": "node",
    "regions": [
        "asia-south1"
    ]
}